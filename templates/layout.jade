!!! 5
html(lang='en')
head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    title SDK Reference
    link(rel='icon', href='http://docs.csharpsdk.org/favicon.ico', type='image/x-icon')
    link(rel='shortcut icon', href='http://docs.csharpsdk.org/favicon.ico', type='image/x-icon')
    meta(property='og:title', content='{{ page.title }}')
    meta(property='og:type', content='article')
    meta(property='og:url', content='http://csharpsdk.org{{ page.url }}')
    meta(property='og:image', content='http://csharpsdk.org/assets/img/logo500x500.png')
    meta(property='og:site_name', content='Facebook SDK for .NET')
    meta(property='fb:admins', content='14812017,605430316,537883665')
    //if lt IE 9
      script(src='http://html5shim.googlecode.com/svn/trunk/html5.js')
    link(href='/assets/css/bootstrap.css', rel='stylesheet')
    link(href='/assets/css/style.css', rel='stylesheet')
    script(type='text/javascript')
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-28972181-1']);
      _gaq.push(['_trackPageview']);
      (function() {
      var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
      ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();
  body
    .navbar.navbar-fixed-top
      a(href='http://github.com/facebook-csharp-sdk/facebook-csharp-sdk')
        img(style='position: absolute; top: 0; right: 0; border: 0;', src='https://s3.amazonaws.com/github/ribbons/forkme_right_darkblue_121621.png', alt='Fork me on GitHub')
      .navbar-inner
        .container
          a.btn.btn-navbar(data-toggle='collapse', data-target='.nav-collapse')
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.brand(href='/')
            strong facebook
            | SDK for .NET
          .nav-collapse
            ul.nav
              li.active
                a(href='/docs') Docs
              li
                a(href='/docs/windows') Windows 8
              li
                a(href='/docs/phone') Windows Phone
              li
                a(href='/docs/web') Web
              li
                a(href='/docs/about') About
    #top.container.content
      .container-body
        .bodyMenu
          #toc
            h5 Topics
            include includes/submenu
        .bodyText
          .header
            h1= name + " Class"
          #doc
            include includes/propertiesSummary
            include includes/methodSummary
            hr
            include includes/details
        .clear
      footer.footer
        .container-footer
          p.pull-right
            a(href='#') Back to top
          p.pull-right
            a(href='/docs/about/#2') License
          p
            | Copyright © 2012 ⋅
            a(href='http://www.outercurve.org') The Outercurve Foundation
            | .
    script(src='/assets/js/jquery-1.7.1.min.js')
    script(src='/assets/js/bootstrap.min.js')
    script(src='/assets/js/prettify.js', type='text/javascript')
    script(type='text/javascript')
      $(document).ready(function() {
      $('code').addClass('prettyprint');
      prettyPrint();
      $('#doc h2').each(function(i, element) {
      var h2 = $(element);
      $('<h6><a href="#' + i + '">' + h2.text() + '</a></h6>').appendTo('#toc');
      h2.after($('<a id="' + i + '" />'));
      });
      });